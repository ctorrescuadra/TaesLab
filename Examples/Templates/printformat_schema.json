{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$ref": "#/definitions/PrintFormat",
    "definitions": {
        "PrintFormat": {
            "type": "object",
            "additionalProperties": false,
            "properties": {               
                "datamodel": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/DataModel"
                    }
                },
                "tables": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Table"
                    }
                },
                "matrices": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Matrix"
                    }
                },
                "summary": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Summary"
                    }
                },
                "format": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Format"
                    }
                }
            },
            "required": [
                "datamodel",
                "tables",
                "matrices",
                "summary",
                "format"
            ],
            "title": "PrintFormat"
        },
        "DataModel": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "number"
                },
                "name": {
                    "type": "string"
                },
                "optional": {
                    "type": "boolean"
                },
                "fields": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/dmField"
                    }
                }
            },
            "required": [
                "id",
                "name",
                "optional",
                "fields"
            ],
            "title": "DataModel"
        },
        "Table": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "integer"
                },
                "key": {
                    "type": "string"
                },
                "number": {
                    "type": "boolean"
                },
                "description": {
                    "type": "string"
                },
                "ttable": {
                    "$ref": "#/definitions/typeTable"
                },
                "resultId": {
                    "$ref": "#/definitions/typeResult"
                },
                "graph": {
                    "$ref": "#/definitions/typeGraph"
                },
                "columns": {
                    "type": "integer"
                },
                "node": {
                    "$ref": "#/definitions/typeNode"
                },
                "rsc": {
                    "type": "boolean"
                },
                "fields": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/tField"
                    }
                }
            },
            "required": [
                "id",
                "key",
                "number",
                "description",
                "ttable",
                "resultId",
                "graph",
                "columns", 
                "node", 
                "rsc",             
                "fields"
            ],
            "title": "Table"
        },
        "Matrix": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "integer"
                },
                "key": {
                    "type": "string"
                },
                "type": {
                    "$ref": "#/definitions/typeFormat"
                },
                "header": {
                    "type": "string"
                },
                "ttable": {
                    "$ref": "#/definitions/typeTable"
                },
                "graph": {
                    "$ref": "#/definitions/typeGraph"
                },
                "options": {
                    "type": "integer",
                    "minimum": 0
                },
                "resultId": {
                    "$ref": "#/definitions/typeResult"
                },
                "rsc": {
                    "type": "boolean"
                },
                "node": {
                    "$ref": "#/definitions/typeNode"
                },
                "rowTotal": {
                    "type": "boolean"
                },
                "colTotal": {
                    "type": "boolean"
                }
            },
            "required": [
                "id",
                "key",
                "header",
                "ttable",
                "type",
                "graph",
                "options",
                "resultId",
                "rsc",
                "rowTotal",
                "colTotal"
            ],
            "title": "Matrix"
        },
        "Summary": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "integer"
                },
                "key": {
                    "type": "string"
                },
                "header": {
                    "type": "string"
                },
                "ttable": {
                    "$ref": "#/definitions/typeTable"
                },
                "node": {
                    "$ref": "#/definitions/typeNode"
                },
                "type": {
                    "$ref": "#/definitions/typeFormat"
                },
                "graph": {
                    "$ref": "#/definitions/typeGraph"
                },
                "options": {
                    "type": "integer",
                    "minimum": 0
                },
                "resultId": {
                   "$ref": "#/definitions/typeResult"
                },
                "stable": {
                    "type": "integer",
                    "minumum" : 1,
                    "maximum" : 2
                },
                "rsc": {
                    "type": "boolean"
                }
            },
            "required": [
                "id",
                "key",
                "header",
                "ttable",
                "node", 
                "type",
                "graph",
                "options",
                "resultId",
                "stable",
                "rsc"
            ],
            "title": "Summary"
        },
        "Format": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "integer"
                },
                "key": {
                    "type": "string"
                },
                "format": {
                    "type": "string"
                },
                "unit": {
                    "type": "string"
                }
            },
            "required": [               
                "id",
                "key",
                "format",
                "unit"
            ],
            "title": "Format"
        },
        "dmField": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                  "type": "number"
                },
                "name": {
                  "type": "string"
                },
                "datatype": {
                    "$ref": "#/definitions/typeData"
                }
            },
            "required": [
                "id",
                "name",
                "datatype"
            ],
            "title": "DataModelField"
        },
        "tField": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "header": {
                    "type": "string"
                },
                "type": {
                    "$ref": "#/definitions/typeFormat"
                }
            },
            "required": [
                "header",
                "name",
                "type"
            ],
            "title": "TableField"
        },
        "typeData": {
            "type": "integer",
            "minimum": 1,
            "maximum": 4
        },
        "typeFormat": {
            "type": "integer",
            "minimum": 1,
            "maximum": 9
        },
        "typeResult": {
            "type": "integer",
            "minimum": 1,
            "maximum": 12
        },
        "typeGraph": {
            "type": "integer",
            "minimum": 0,
            "maximum": 8
        },
        "typeNode": {
            "type": "integer",
            "minumum" : 0,
            "maximum" : 4
        },
        "typeTable": {
            "type": "integer",
            "minumum" : 0,
            "maximum" : 4
        }
    }
}
