            <product>V4-V3</product>
            <type>PRODUCTIVE</type>
        </processes>
        <processes>
            <key>GEN</key>
            <description>Generator</description>
            <fuel>WGT+WST</fuel>
            <product>WN+WCP</product>
            <type>PRODUCTIVE</type>
        </processes>
        <processes>
            <key>REF</key>
            <description>Refrigerator</description>
            <fuel>G6-G7</fuel>
            <product>COLD</product>
