        <processes>
            <key>HE</key>
            <description>Heater</description>
            <fuel>G7-G8</fuel>
            <product>HEAT</product>
            <type>PRODUCTIVE</type>
        </processes>
        <processes>
            <key>STCK</key>
            <description>Stack</description>
            <fuel>G5+G8</fuel>
            <product>G9</product>
            <type>DISSIPATIVE</type>
        </processes>
        <processes>
